    "h3": "^1.5.0",
    "jsdom": "^20.0.3",
    "nitropack": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "pug": "^3.0.2",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.7.4",
    "unplugin-vue-router": "^0.5.0",
    "vite": "^4.0.0",
