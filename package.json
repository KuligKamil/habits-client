    "@antfu/eslint-config": "^0.35.2",
    "@mdi/font": "^7.1.96",
    "@pinia/testing": "^0.0.15",
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@types/testing-library__jest-dom": "^5.14.5",
